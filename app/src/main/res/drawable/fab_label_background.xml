<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

  <item>
    <shape
      android:dither="true"
      android:shape="rectangle">

      <corners android:radius="@dimen/fab_label_corners" />
      <solid android:color="?attr/colorFabLabelBackground" />

      <padding
        android:bottom="@dimen/fab_label_padding_vertical"
        android:left="@dimen/fab_label_padding_horizontal"
        android:right="@dimen/fab_label_padding_horizontal"
        android:top="@dimen/fab_label_padding_vertical" />

    </shape>
  </item>

</layer-list>
